{
  "name": "@kb-labs/tox-adapters",
  "description": "Adapters for KB Labs TOX - Toolchain for Object eXchange",
  "author": {
    "name": "Kirill Baranov",
    "url": "https://github.com/kirill-baranov"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kirill-baranov/kb-labs-tox.git"
  },
  "bugs": {
    "url": "https://github.com/kirill-baranov/kb-labs-tox/issues"
  },
  "homepage": "https://github.com/kirill-baranov/kb-labs-tox#readme",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run -c ../../vitest.config.ts",
    "test:watch": "vitest -c ../../vitest.config.ts"
  },
  "dependencies": {
    "@kb-labs/tox-core": "link:../tox-core",
    "@kb-labs/tox-codec-json": "link:../tox-codec-json",
    "@kb-labs/mind-types": "link:../../../kb-labs-mind/packages/mind-types"
  },
  "devDependencies": {
    "tsx": "^4.20.5",
    "vitest": "^3",
    "@kb-labs/devkit": "github:KirillBaranov/kb-labs-devkit#main",
    "tsup": "^8",
    "@types/node": "^24.7.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  }
}

